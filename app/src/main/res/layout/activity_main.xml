<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="165dp"
        android:layout_height="96dp"
        android:onClick="TablefrmOut"
        android:text="@string/button_finbill"
        tools:layout_editor_absoluteX="535dp"
        tools:layout_editor_absoluteY="626dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="337dp"
        android:layout_height="89dp"
        android:ems="10"
        android:hint="@string/edit_totalbill"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="186dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="332dp"
        android:layout_height="82dp"
        android:ems="10"
        android:hint="@string/print_change"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="321dp"
        tools:layout_editor_absoluteY="323dp" />
</androidx.constraintlayout.widget.ConstraintLayout>